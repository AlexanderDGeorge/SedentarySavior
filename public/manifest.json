{
  "manifest_version": 2,
  "name": "Sedentary Savior",
  "version": "0.1.0",
  "background": {
    "scripts": [
      "configuration.js",
      "Notification.js",
      "optionalPermissions.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": ["push", "notifications"],
  "optional_permissions": ["tabs"]
}
